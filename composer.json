{
    "name": "bloghunch/php-sdk",
    "description": "A PHP SDK for the Bloghunch API",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "autoload": {
        "psr-4": {
            "Bloghunch\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bloghunch",
            "email": "support@bloghunch.com"
        }
    ],

    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
